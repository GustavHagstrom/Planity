@page "/"

<PageTitle>Dashboard</PageTitle>

<div class="pa-2">
    <h1>Dashboard</h1>

    <h3 style="margin-top: 20px">Routes</h3>
    @foreach (var route in Routes.AllRoutes)
    {
        <a href="@route"><h5>@route</h5></a>
    }
</div>

<GanttView Start="DateTime.Today" End="DateTime.Today.AddDays(40)">
    
</GanttView>

