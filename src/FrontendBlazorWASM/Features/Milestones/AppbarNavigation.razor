@inject IStringLocalizer<AppbarNavigation> Localizer

<FeatureNavigationMenu FeatureRoute="@Routes.MilestonesOverview" MenuTitle="@Localizer["Milstenar"]" Match="NavLinkMatch.Prefix" />


@code {
    List<MenuLink> menuLinks = new();

    protected override void OnInitialized()
    {
        menuLinks = new()
        {
            new MenuLink(Localizer["Översikt"], Routes.MilestonesOverview, null, NavLinkMatch.All),
        };
    }
}
