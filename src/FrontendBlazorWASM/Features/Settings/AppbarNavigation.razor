@inject IStringLocalizer<AppbarNavigation> Localizer

<FeatureNavigationMenu FeatureRoute="@Routes.Settings" MenuTitle="@Localizer["Inställningar"]" Match="NavLinkMatch.Prefix" />


@code {
    List<MenuLink> menuLinks = new();

    protected override void OnInitialized()
    {
        menuLinks = new()
        {
            new MenuLink(Localizer["Översikt"], Routes.ProjectsOverview, null, NavLinkMatch.All),
        };
    }
}
