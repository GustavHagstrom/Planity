@inject IStringLocalizer<AppbarNavigation> Localizer

<FeatureNavigationMenu FeatureRoute="@Routes.ResourcesOverview" MenuTitle="@Localizer["Resurser"]" Match="NavLinkMatch.Prefix"/>


@code {
    List<MenuLink> menuLinks = new();

    protected override void OnInitialized()
    {
        menuLinks = new()
        {
            new MenuLink(Localizer["Översikt"], Routes.ResourcesOverview, null, NavLinkMatch.All),

        };
    }
}
