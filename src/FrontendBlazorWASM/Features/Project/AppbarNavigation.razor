@inject IStringLocalizer<AppbarNavigation> Localizer

<FeatureNavigationMenu FeatureRoute="@Routes.ProjectsOverview" MenuTitle="@Localizer["Projekt"]" Match="NavLinkMatch.Prefix"/>


@code {
    List<MenuLink> menuLinks = new();

    protected override void OnInitialized()
    {
        menuLinks = new()
        {
            new MenuLink(Localizer["Översikt"], Routes.ProjectsOverview, null, NavLinkMatch.All),
        };
    }
}
