@using System.Globalization

<polyline points="@Points" stroke="@StrokeColor" stroke-width="@StrokeWidth" marker-end="@MarkerEnd" style="pointer-events:none;fill:none;" />

@code {
    [Parameter] public string Points { get; set; } = string.Empty;
    [Parameter] public string StrokeColor { get; set; } = "#3a6ea5";
    [Parameter] public double StrokeWidth { get; set; } = 2;
    [Parameter] public string MarkerEnd { get; set; } = "url(#arrowhead)";
}
